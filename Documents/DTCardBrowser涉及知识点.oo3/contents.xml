                  <values>
                    <text>
                      <p>
                        <run>
                          <lit>final</lit>
                        </run>
                        <run>
                          <lit>关键字可用在</lit>
                        </run>
                        <run>
                          <lit>class</lit>
                        </run>
                        <run>
                          <lit>、</lit>
                        </run>
                        <run>
                          <lit>func</lit>
                        </run>
                        <run>
                          <lit>或者</lit>
                        </run>
                        <run>
                          <lit>var</lit>
                        </run>
                        <run>
                          <lit>前面进行修饰，表示不允许对该内容进行继承或者重写操作。</lit>
                        </run>
                      </p>
                    </text>
